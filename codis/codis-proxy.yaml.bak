apiVersion: v1
kind: ReplicationController
metadata:
  namespace: dingxin
  name: codis-proxy
spec:
  replicas: 2
  template:
    metadata:
      labels:
        app: codis-proxy
    spec:
      containers:
      - name: codis-proxy
        image: harbor-ui:30814/dingchat/dx-codis-proxy:3.0.0 
        imagePullPolicy: IfNotPresent
        env:
        - name: COORDINATOR_NAME
          value: "zookeeper"
        - name: COORDINATOR_ADDR 
          value: "zk-cs:2181"
        - name: CODIS_DASHBOARD_ADDR
          value: "codis-dashboard:18080"
        - name: PRODUCT_NAME
          value: "codis-dingxin"
        #- name: PRODUCT_AUTH
        #        #  value: ""
        ports:
        - containerPort: 11080
          name: proxy
        - containerPort: 19000
          name: admin
        #resources:
        # limits:
        #   cpu: "4"
        #   memory: 4Gi
        # requests:
        #   cpu: "0.1"
        #   memory: 0.1Gi
